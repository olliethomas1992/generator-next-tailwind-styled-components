{
	"presets": [
		"next/babel",
		"@babel/preset-env",
		"@babel/preset-react"
	],
	"env": {
		"production": {
			"plugins": [
				[
					"emotion",
					{
						"hoist": true
					}
				],
				"tailwind-components"
			]
		},
		"development": {
			"plugins": [
				[
					"emotion",
					{
						"sourceMap": true,
						"autoLabel": true
					}
				],
				"tailwind-components"
			]
		}
	}
}